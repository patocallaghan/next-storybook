{
  "name": "next-storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "snapshot:storybook": "build-storybook && yarn percy exec --parallel yarn percy:storybook",
    "percy:storybook": "percy storybook ./storybook-static --include='Default Button' --quiet",
    "snapshot:cypress": "yarn percy exec --parallel yarn cypress:run",
    "test": "start-server-and-test start http://localhost:3000 snapshot:cypress"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@percy-io/in-percy": "^0.1.11",
    "@percy/cli": "^1.0.0-beta.60",
    "@percy/cypress": "^3.1.1",
    "@percy/storybook": "^4.0.0-beta.2",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@types/react": "17.0.15",
    "babel-loader": "^8.2.2",
    "cypress": "^8.0.0",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "start-server-and-test": "^1.13.1",
    "typescript": "4.3.5"
  }
}
